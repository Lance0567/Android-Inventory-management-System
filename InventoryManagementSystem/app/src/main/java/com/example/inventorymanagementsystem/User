package com.example.inventorymanagementsystem;

import java.util.Comparator;

public class User {

    public String fullName, age, email, serialNumber, section, laptop;

    public String getFirstName() {
        return fullName;
    }

    public String getSerialNumber() {
        return serialNumber;
    }

    public String getSection() {
        return section;
    }

    public String getLaptop() {
        return laptop;
    }

    public String getEmail() {
        return email;
    }

    public User(){

    }

    public User(String fullName, String age, String email, String section, String serialNumber, String laptop){
        this.fullName = fullName;
        this.age = age;
        this.email = email;
        this.serialNumber = serialNumber;
        this.section = section;
        this.laptop = laptop;
    }
    public static Comparator<User> myName = new Comparator<User>() {
        @Override
        public int compare(User user, User t1) {
            return 0;
        }
    };
}
